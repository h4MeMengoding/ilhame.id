{
  "functions": {
    "src/pages/api/chat.ts": {
      "memory": 1024,
      "maxDuration": 10
    },
    "src/pages/s/[slug].tsx": {
      "memory": 256,
      "maxDuration": 8
    },
    "src/pages/api/r/[slug].ts": {
      "memory": 128,
      "maxDuration": 5
    }
  },
  "rewrites": [
    {
      "source": "/s/:slug",
      "destination": "/api/r/:slug"
    }
  ]
}
