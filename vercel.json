{
  "functions": {
    "src/pages/api/chat.ts": {
      "memory": 1024,
      "maxDuration": 10
    },
    "src/pages/s/[slug].tsx": {
      "memory": 256,
      "maxDuration": 8
    },
    "src/pages/api/r/[slug].ts": {
      "memory": 256,
      "maxDuration": 5
    },
    "src/pages/api/fast/[slug].ts": {
      "memory": 256,
      "maxDuration": 5
    },
    "src/pages/api/direct/[slug].ts": {
      "memory": 256,
      "maxDuration": 5
    },
    "src/pages/api/ultra-fast/[slug].ts": {
      "memory": 256,
      "maxDuration": 5
    }
  },
  "headers": [
    {
      "source": "/api/(direct|fast|r)/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, s-maxage=300"
        }
      ]
    }
  ]
}
